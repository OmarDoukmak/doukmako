<?xml version="1.0" ?>
<odoo>
    <record id="tds_inherit_view_form" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.tds</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="hype_cable_pro.view_order_form_cable"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Technical Data Sheet">
                    <center>
                        <field name="cable_2d_image" widget="image"/>
                    </center>
<!--                    <hr/>-->
                    <field name="tds_html" readonly="1"/>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
